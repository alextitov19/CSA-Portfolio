<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="Gmail.GmailUI">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="17c1e" class="javax.swing.JTextArea" binding="recipientTextArea">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag top="5" left="5" bottom="5" right="5" weightx="1.0" weighty="0.1"/>
        </constraints>
        <properties>
          <text value="email@email.com"/>
        </properties>
      </component>
      <component id="dfefc" class="javax.swing.JLabel" binding="recipientLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag top="5" left="5" bottom="5" right="5" weightx="0.1" weighty="0.1"/>
        </constraints>
        <properties>
          <horizontalAlignment value="11"/>
          <text value="Please enter your email"/>
        </properties>
      </component>
      <component id="3d129" class="javax.swing.JButton" binding="sendButton">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag top="5" left="5" bottom="5" right="5" weightx="0.1" weighty="0.1"/>
        </constraints>
        <properties>
          <text value="Send"/>
        </properties>
      </component>
    </children>
  </grid>
</form>